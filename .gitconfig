[user]
    name = "Matt Hems"
    email = mhems1@binghamton.edu

[init]
    templatedir = ~/.git_template

[core]
    editor = emacs
    whitespace = blank-at-eol, space-before-tab
    excludesfile = ~/.gitignore
    pager = less -F

[color]
    ui = auto

[diff]
    tool = meld

[difftool]
    prompt = false

[github]
    user = mhems

[alias]
    co = checkout
    cob = checkout -b
    ci = commit
    cim = commit -m
    cam = commit -am
    amend = commit --no-edit --amend
    br = branch
    st = status
    d = diff
    di = diff --ignore-all-space
    up = push origin master
    dl = pull origin master
    view = log origin/master HEAD master --graph --decorate --oneline
    v = log HEAD master --graph --decorate --oneline
    last = log -1 HEAD

