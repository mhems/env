[alias]
    amend = commit --no-edit --amend
    br = branch
    cam = commit -am
    ci = commit
    cim = commit -m
    co = checkout
    cob = checkout -b
    d = diff
    di = diff --ignore-all-space
    dl = pull origin master
    iadd = add --patch
    last = log -1 HEAD
    pl = git pull
    pr = git pull --rebase
    ps = git push
    st = status
    uncommit = reset --soft HEAD
    unstage = reset HEAD
    up = push origin master
    v = log HEAD master --graph --decorate --oneline
    view = log origin/master HEAD master --graph --decorate --oneline

[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = auto

[core]
    editor = emacs
    excludesfile = ~/.gitignore
    pager = less -F
    whitespace = blank-at-eol, space-before-tab

[credential]
    helper = cache

[diff]
    tool = meld

[difftool]
    prompt = false

[gc]
    auto = 1

[github]
    user = mhems

[init]
    templatedir = ~/.git_template

[merge]
    summary = true

[push]
    default = simple

[user]
    email = mhems1@binghamton.edu
    name = "Matt Hems"
